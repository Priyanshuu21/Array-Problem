#include <iostream>
using namespace std;
int main(){
    int arr[5][5];

    cout<<"Enter elements in array : "<<endl;
    for(int i=0;i<5;i++){
        for(int j=0;j<5;j++){
            cin>>arr[i][j];
        }
    }

    cout<<"Elements store in array : "<<endl;
    for(int i=0;i<5;i++){
        for(int j=0;j<5;j++){
            cout<<arr[i][j]<<" ";
        }
        cout<<endl;
    }

    int sum = 0;
    for(int i=0;i<5;i++){
        for(int j=0;j<5;j++){
            sum = sum + arr[i][j];
        }
        cout<<"Sum of row elements are : "<<sum<<endl;
        sum = 0;
    }
}
