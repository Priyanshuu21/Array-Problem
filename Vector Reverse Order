#include <iostream>
#include <vector>
using namespace std;
int main(){
    int n;
    cout<<"Enter size of container :"<<endl;
    cin>>n;

    vector<int> arr(n);
    for(int i=0;i<arr.size();i++){
        cin>>arr[i];
        cout<<arr[i]<<" "<<endl;
    }

    cout<<"In reverse order,your container looks like :"<<endl;
    for(int i=arr.size()-1;i>=0;i--){
        cout<<arr[i]<<" "<<endl;
    }
}
