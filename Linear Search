#include<iostream>
using namespace std;
int main(){
    int arr[10] = {2,4,5,6,7,89,12,14,10,124};

    int num;
    cout<<"Search Element Here!";
    cin>>num;

    bool flag = 0;

    for(int i=0;i<10;i++){
        if(arr[i] == num){
            flag = 1;
        }
    }
    if (flag == 1){
        cout<<"Elements Found";
    }
    else{
        cout<<"Elements Not Found!";
    }
    return 0;
}
