#include <iostream>
#include <limits.h>
using namespace std;
int main(){

    int arr[] = {1,2,34,567,88,0,23};
    int size = 7;
    int min = INT_MAX;

    for(int i=0;i<size;i++){
        if(arr[i] < min){
            min = arr[i];
        }
    }
    cout<<"Minimum number in given array is :"<<min;
    return 0;
}
